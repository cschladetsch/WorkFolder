#!/bin/bash

projectname=$1

cd $WORK_DIR
mkdir $projectname
cd $projectname

echo "# Readme" > Readme.md

cp $WORK_DIR/doc/gitignore-std .gitignore

git init .
git remote add origin git@github.com:LiminalVR/$projectname.git

git add .gitignore
git add *
git commit -m "Created"

git push -u origin master
