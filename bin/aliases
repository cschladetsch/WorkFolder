# Common aliases

alias h='history'
alias g='git'
alias gll='g lg'
alias gl='gll | head -5'
alias gd='g diff'
alias gf='g fetch'
alias gp='g push'
alias gu='g pull'
alias glog='g reflog'
alias gch='g checkout'
alias gc='g commit -m'
alias gr='g remote -v'
alias gppu='gp && gpu'
alias gpu='gp upstream'
alias grhh='g reset --hard HEAD'
alias gs='g status --short --branch'
alias gshow='g show --name-only --oneline'
alias hi='hub issue --color'

alias cdw='cd /w'
alias cdp='cd /w/Packages'
alias cdr='cd /w/repos'

echo_verbose 2 "...aliases"

